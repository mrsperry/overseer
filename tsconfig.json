{
    "compilerOptions": {
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": false,
        "esModuleInterop": true,
        "sourceMap": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "resolveJsonModule": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "jsx": "react",
        "allowJs": false
    }
}